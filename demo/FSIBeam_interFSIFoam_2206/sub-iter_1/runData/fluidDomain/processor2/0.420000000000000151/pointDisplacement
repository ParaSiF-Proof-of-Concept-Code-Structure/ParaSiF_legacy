FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       pointVectorField;
    location    "0.420000000000000151";
    object      pointDisplacement;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform List<vector> 0();
    }
    outlet
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    top
    {
        type            slip;
    }
    bottom
    {
        type            fixedValue;
        value           nonuniform List<vector> 0();
    }
    left
    {
        type            slip;
    }
    symmetry
    {
        type            symmetryPlane;
    }
    interface
    {
        type            fixedValue;
        value           nonuniform List<vector> 0();
    }
    procBoundary2to1
    {
        type            processor;
    }
    procBoundary2to3
    {
        type            processor;
    }
}


